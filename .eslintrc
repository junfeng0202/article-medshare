{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "global-require": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "no-new": "off",
    "func-names": "off",
    "linebreak-style": 0
  },
  "globals": {
    "Vue": true
  }
}